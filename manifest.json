{
    "manifest_version": 3,
    "name": "Base64decoder",
    "description": "Decode Base64 and open the url",
    "version": "1.0",
    "permissions": ["contextMenus", "tabs"],
    "background": {
        "service_worker": "service.js"
    },
    "content_scripts" : [
        {
            "matches": ["<all_urls>"],
            "js": ["/content.js"]
        }
    ]
  }